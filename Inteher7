#include <iostream>

using namespace std;

int main() {
    int num, first, second, sum, product;

    cout << "Введите двузначное число: ";
    cin >> num;

    first = num / 10; 
    second = num % 10; 

    sum = first + second;
    product = first * second; 

    cout << "Сумма цифр: " << sum << endl;
    cout << "Произведение цифр: " << product << endl;

    return 0;
